    "test": "jest --watch",
    "test:ci": "jest --ci",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "prisma": "^4.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.2",
    "dotenv": "^16.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-extended": "3.0.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} packages/prisma/seed/index.ts",
    "schema": "packages/prisma/schema.prisma"
  }
}
