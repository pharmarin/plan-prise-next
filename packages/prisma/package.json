  },
  "devDependencies": {
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} packages/prisma/seed/index.ts",
    "schema": "packages/prisma/schema.prisma"
  }
}
