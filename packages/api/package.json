{
  "name": "@plan-prise/api",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./constants": "./src/constants.ts",
    "./utils/*": "./src/utils/*",
    "./validation/*": "./src/validation/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@plan-prise/api-pharmaciens": "*",
    "@plan-prise/auth": "*",
    "@plan-prise/db-prisma": "*",
    "@plan-prise/errors": "*",
    "@trpc/server": "10.43.6",
    "jose": "4.15.4",
    "mailersend": "2.2.0",
    "superjson": "1.13.3",
    "yup": "1.3.2",
    "yup-locales": "1.2.20",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@plan-prise/eslint-config": "0.2.0",
    "@plan-prise/prettier-config": "^0.1.0",
    "@plan-prise/tsconfig": "^0.1.0",
    "eslint": "8.54.0",
    "typescript": "5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "@plan-prise/eslint-config/base"
    ]
  },
  "prettier": "@plan-prise/prettier-config"
}
