{
  "name": "@plan-prise/api",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./constants": "./src/constants.ts",
    "./utils/*": "./utils/*.ts",
    "./validation/*": "./validation/*.ts"
  },
  "typesVersions": {
    "*": {
      "utils/*": [
        "utils/*"
      ],
      "validation/*": [
        "validation/*"
      ],
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "@plan-prise/api-pharmaciens": "*",
    "@plan-prise/auth": "*",
    "@plan-prise/db-prisma": "*",
    "@plan-prise/errors": "*",
    "@t3-oss/env-nextjs": "0.9.2",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "10.45.1",
    "@trpc/react-query": "10.45.1",
    "@trpc/server": "10.45.1",
    "@types/lodash": "4.14.202",
    "i18next": "23.10.0",
    "jose": "4.15.4",
    "lodash": "4.17.21",
    "mailersend": "2.2.0",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "2.2.1",
    "zod": "3.22.4",
    "zod-i18n-map": "2.27.0"
  },
  "devDependencies": {
    "@plan-prise/eslint-config": "0.2.0",
    "@plan-prise/prettier-config": "^0.1.0",
    "@plan-prise/tsconfig": "^0.1.0",
    "eslint": "8.56.0",
    "typescript": "5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@plan-prise/eslint-config/base"
    ]
  },
  "prettier": "@plan-prise/prettier-config"
}
