    "@plan-prise/auth": "*",
    "@plan-prise/db-prisma": "*",
    "@plan-prise/errors": "*",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "@types/lodash": "4.14.202",
    "i18next": "23.7.15",
    "jose": "4.15.4",
    "lodash": "4.17.21",
    "mailersend": "2.2.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.3",
    "yup": "1.3.3",
