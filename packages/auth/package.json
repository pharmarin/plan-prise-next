    "@plan-prise/errors": "0.1.0",
    "@prisma/client": "5.11.0",
    "@t3-oss/env-nextjs": "0.9.2",
    "bcryptjs": "2.4.3",
    "next": "14.1.1",
    "next-auth": "4.24.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@plan-prise/eslint-config": "0.2.0",
    "@plan-prise/prettier-config": "0.1.0",
    "@plan-prise/tsconfig": "0.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/react": "18.2.69",
    "eslint": "8.57.0",
    "typescript": "5.4.3"
  },
  "eslintConfig": {
    "extends": [
      "@plan-prise/eslint-config/base"
