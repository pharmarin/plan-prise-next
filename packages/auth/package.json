{
  "name": "@plan-prise/auth",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./*": "./src/*.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
