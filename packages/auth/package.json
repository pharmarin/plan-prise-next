    "react-dom": "18.2.0",
    "server-only": "0.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@plan-prise/eslint-config": "0.2.0",
    "@plan-prise/prettier-config": "0.1.0",
    "@plan-prise/tsconfig": "0.1.0",
