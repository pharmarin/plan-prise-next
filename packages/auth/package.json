{
  "name": "@plan-prise/auth",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts",
    "./*": "./src/*.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "1.0.7",
    "@plan-prise/db-prisma": "workspace:*",
    "@plan-prise/errors": "workspace:*",
    "@prisma/client": "5.18.0",
    "@t3-oss/env-nextjs": "0.11.1",
    "bcryptjs": "2.4.3",
    "next": "14.2.7",
    "next-auth": "4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "server-only": "0.0.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@plan-prise/eslint-config": "workspace:*",
    "@plan-prise/prettier-config": "workspace:*",
    "@plan-prise/tsconfig": "workspace:*",
    "@types/bcryptjs": "2.4.6",
    "@types/react": "18.3.4",
    "eslint": "8.57.0",
    "typescript": "5.4.5"
  },
  "eslintConfig": {
    "extends": [
      "@plan-prise/eslint-config/base"
    ]
  },
  "prettier": "@plan-prise/prettier-config"
}
