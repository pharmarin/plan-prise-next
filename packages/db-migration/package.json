    "@types/cli-progress": "3.11.5",
    "@types/lodash": "4.14.202",
    "cli-progress": "3.12.0",
    "eslint": "8.54.0",
    "lodash": "4.17.21",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.2"
