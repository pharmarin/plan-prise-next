{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "**/.env",
    ".env"
  ],
  "tasks": {
    "build": {
      "env": ["DATABASE_URL_POSTGRE", "BACKEND_URL"],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".expo/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "env": ["DATABASE_URL_POSTGRE", "BACKEND_URL"],
      "outputs": [
        "node_modules/.cache/.eslintcache"
      ]
    },
    "pretest": {
      "cache": false
    },
    "start": {
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "pretest"
      ],
      "outputs": [
        "playwright-report/**"
      ]
    },
    "typecheck": {
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  }
}
