{
  "extends": "@plan-prise/tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
    },
    "plugins": [
      {
        "name": "next",
      },
    ],
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
  },
  "include": [".", ".next/types/**/*.ts"],
  "exclude": ["node_modules", "playwright-report", "__tests__"],
}
