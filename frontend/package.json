{
  "name": "@plan-prise/app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "env:generate": "gen-env-types .env",
    "format": "prettier --check \"**/*.{mjs,ts,md,json,tsx}\"",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "lint:fix": "pnpm lint --fix",
    "start": "pnpm with-env next start",
    "pretest": "playwright install --with-deps",
    "test": "playwright test __tests__/base/",
    "test:base": "playwright test __tests__/base/",
    "test:e2e": "playwright test __tests__/e2e/",
    "test:utils": "playwright test __tests__/utils/",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../.env --"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.3",
    "@paralleldrive/cuid2": "2.2.2",
    "@plan-prise/api": "0.1.0",
    "@plan-prise/auth": "0.1.0",
    "@plan-prise/db-prisma": "0.1.0",
    "@plan-prise/errors": "0.1.0",
    "@plan-prise/eslint-config": "0.2.0",
    "@plan-prise/prettier-config": "0.1.0",
    "@plan-prise/tailwind-config": "0.1.0",
    "@plan-prise/tsconfig": "0.1.0",
    "@plan-prise/ui": "0.1.0",
    "@prisma/client": "5.7.1",
    "@react-pdf/renderer": "3.1.14",
    "@sentry/nextjs": "7.91.0",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-table": "8.11.2",
    "@tremor/react": "3.12.1",
    "@trpc/client": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "formik": "2.4.5",
    "immer": "10.0.3",
    "lodash": "4.17.21",
    "lucide-react": "0.302.0",
    "next": "14.0.4",
    "next-auth": "4.24.5",
    "next-superjson-plugin": "0.5.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-hook-form": "7.49.2",
    "react-pdf-html": "1.1.18",
    "react-pdf-tailwind": "2.2.1",
    "react-select": "5.8.0",
    "superjson": "1.13.3",
    "tailwind-merge": "2.2.0",
    "typescript": "5.3.3",
    "zod": "3.22.4",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@next/env": "14.0.4",
    "@playwright/test": "1.40.1",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.5",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "dotenv-cli": "7.3.0",
    "gen-env-types": "1.3.4",
    "postcss": "8.4.32",
    "prisma": "5.7.1",
    "raw-loader": "4.0.2",
    "tailwindcss": "3.4.0",
    "webpack": "5.89.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@plan-prise/eslint-config/base",
      "@plan-prise/eslint-config/nextjs",
      "@plan-prise/eslint-config/react"
    ]
  }
}
